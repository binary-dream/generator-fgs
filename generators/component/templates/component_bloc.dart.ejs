<%- importPackages %>

class <%= screenNameUpperCase %>__<%= componentNameUpperCase %>__ComponentBloc 
  extends
    Bloc<
      <%= screenNameUpperCase %>__<%= componentNameUpperCase %>__ComponentEvent, 
      <%= screenNameUpperCase %>__<%= componentNameUpperCase %>__ComponentState
    > {
  <%= screenNameUpperCase %>__<%= componentNameUpperCase %>__ComponentBloc({
    required this.componentData,
    required <%= screenNameUpperCase %>__<%= componentNameUpperCase %>__ComponentState initialState,
    required this.uuidProvider,
    required this.analyticsProvider,
    required this.logProvider,
    // --- EVENT HANDLERS ON CONSTRUCTOR BEGIN ---------------------------------
    required this.onComponentStartedEventHandler,
    // --- EVENT HANDLERS ON CONSTRUCTOR END -----------------------------------
  }) : super(initialState) {
    // --- EVENTS BEGIN --------------------------------------------------------
    on<<%= screenNameUpperCase %>__<%= componentNameUpperCase %>__ComponentEvent__ComponentStarted>(
      (event, emit) async {
        await onComponentStartedEventHandler.execute(
          bloc: this,
          event: event,
          emit: emit,
        );
      },
    );
    // --- EVENTS END ----------------------------------------------------------
  }

  final <%= screenNameUpperCase %>__<%= componentNameUpperCase %>__ComponentData componentData;
  final FGS_UTILS__UuidProvider uuidProvider;
  final FGS_UTILS__AnalyticsProvider analyticsProvider;
  final FGS_UTILS__LogProvider logProvider;
  // --- EVENT HANDLERS DECLARATIONS BEGIN -------------------------------------
  final <%= screenNameUpperCase %>__<%= componentNameUpperCase %>__OnComponentStartedEventHandler 
    onComponentStartedEventHandler;
  // --- EVENT HANDLERS DECLARATIONS END ---------------------------------------
}
