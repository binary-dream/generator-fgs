<%- importPackages %>

class <%= screenNameUpperCase %>__<%= componentNameUpperCase %>__ComponentBloc 
  extends
    Bloc<
      <%= screenNameUpperCase %>__<%= componentNameUpperCase %>__ComponentEvent, 
      <%= screenNameUpperCase %>__<%= componentNameUpperCase %>__ComponentState
    > {
  <%= screenNameUpperCase %>__<%= componentNameUpperCase %>__ComponentBloc({
    required this.componentData,
    required <%= screenNameUpperCase %>__<%= componentNameUpperCase %>__ComponentState initialState,
    required this.uuidService,
    required this.analyticsService,
    required this.logService,
  }) : super(initialState) {
    // --- EVENTS BEGIN --------------------------------------------------------
    on<<%= screenNameUpperCase %>__<%= componentNameUpperCase %>__ComponentEvent__ComponentStarted>(
      (event, emit) async {
        await <%= screenNameUpperCase %>__<%= componentNameUpperCase %>__onComponentStartedEventHandler(
          bloc: this,
          event: event,
          emit: emit,
        );
      },
    );
    // --- EVENTS END ----------------------------------------------------------
  }

  final <%= screenNameUpperCase %>__<%= componentNameUpperCase %>__ComponentData componentData;
  final FGS_UTILS__UuidService uuidService;
  final FGS_UTILS__AnalyticsService analyticsService;
  final FGS_UTILS__LogService logService;
}
