<%- importPackages %>

class <%= screenNameUpperCase %>__ScreenBloc 
  extends Bloc<
    <%= screenNameUpperCase %>__ScreenEvent, 
    <%= screenNameUpperCase %>__ScreenState
  > {
  <%= screenNameUpperCase %>__ScreenBloc({
    required this.screenData,
    required <%= screenNameUpperCase %>__ScreenState initialState, 
    required this.uuidService,
    required this.analyticsService,
    required this.logService,
  }) : super(initialState) {
    // --- EVENTS BEGIN --------------------------------------------------------
    on<<%= screenNameUpperCase %>__ScreenEvent__ScreenStarted>(
      (event, emit) async {
        await <%= screenNameUpperCase %>__onScreenStartedEventHandler(
          bloc: this,
          event: event,
          emit: emit,
        );
      },
    );
    // --- EVENTS END ----------------------------------------------------------
  }

  final <%= screenNameUpperCase %>__ScreenData screenData;
  final FGS_UTILS__UuidService uuidService;
  final FGS_UTILS__AnalyticsService analyticsService;
  final FGS_UTILS__LogService logService;
}
